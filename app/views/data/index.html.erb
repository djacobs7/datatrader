<h1>Listing data</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Name</th>
    <th>Schema</th>
    <th>Data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @data.each do |datum| %>
  <tr>
    <td><%= datum.description %></td>
    <td><%= datum.name %></td>
    <td><%= datum.schema %></td>
    <td><%= datum.data %></td>
    <td><%= link_to 'Show', datum %></td>
    <td><%= link_to 'Edit', edit_datum_path(datum) %></td>
    <td><%= link_to 'Destroy', datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Datum', new_datum_path %>
